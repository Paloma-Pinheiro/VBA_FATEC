Option Explicit

Sub triangulo()

# Receba a base e a altura de um tri칙ngulo. Calcule e mostre a sua 치rea.

Dim base As Double
Dim altura As Double
Dim area As Double

base = InputBox("Insira a base dos tri칙ngulo")
altura = InputBox("Insira a altura do tri칙ngulo")

area = (base * altura) / 2

MsgBox (area)
End Sub
